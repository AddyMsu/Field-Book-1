<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="15.0dip" />
            <gradient
                android:angle="270.0"
                android:centerColor="?attr/fb_trait_percent_background_center_color"
                android:centerY="0.5"
                android:endColor="?attr/fb_trait_percent_background_start_end_color"
                android:startColor="?attr/fb_trait_percent_background_start_end_color" />
            <stroke
                android:width="1.0dip"
                android:color="?attr/fb_trait_percent_stroke_color" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="15.0dip" />
                <gradient
                    android:angle="90.0"
                    android:endColor="?attr/fb_trait_percent_stroke_color"
                    android:startColor="?attr/fb_trait_percent_start_color" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="15.0dip" />
                <gradient
                    android:angle="90.0"
                    android:endColor="?attr/fb_trait_percent_stroke_color"
                    android:startColor="?attr/fb_trait_percent_start_color" />
            </shape>
        </clip>
    </item>
</layer-list>